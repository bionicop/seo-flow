# === Data Sources ===
SERPER_API_KEY=your_serper_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
GSC_CREDENTIALS_PATH=./credentials/gsc_service_account.json

# === Defaults ===
DEFAULT_DATA_SOURCE=serper
